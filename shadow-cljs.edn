{:deps   true
 :builds {:app {:target     :browser
                :asset-path "/js"
                :output-dir "resources/public/js"
                :modules    {:main {:init-fn user/start!
                                    :entries [user]}}
                :devtools   {:watch-dir       "resources/public"
                             :hud             #{:errors :progress}
                             :ignore-warnings true}}}
 :nrepl  {:port 9001}}
